pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello from Github hook trigger'
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
            }
        }        
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }        
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }    
        stage('Release') {
            steps {
                echo 'Releasing'
            }
        }    
    }
}
